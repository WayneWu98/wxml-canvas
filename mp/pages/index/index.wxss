.example-list {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}

.link {
  background-color: #f5f5f5;
  line-height: 80rpx;
  display: block;
  text-align: center;
  height: 80rpx;
  width: 320rpx;
  font-size: 32rpx;
  border-radius: 16rpx;
  margin-bottom: 16rpx;
}

.heading {
  margin-bottom: 32rpx;
  font-size: 32rpx;
  opacity: .7;
}